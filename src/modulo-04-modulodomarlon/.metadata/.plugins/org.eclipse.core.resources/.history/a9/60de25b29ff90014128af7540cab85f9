import java.util.ArrayList;

public class Filme {
	private ArrayList<Ator> elenco = new ArrayList<>();
	private String titulo;
	
	public Filme(String titulo){
		this.titulo=titulo;
	}
	
	public String toString(){
		String aux = "Titulo: ".concat(this.titulo.concat("\nElenco:"));
		for(Ator ator: elenco){
			aux.concat(" ".concat(ator.toString()));
		}
		return aux;
	}
	private void inserirAtor(Ator ator){
		this.elenco.add(ator);
	}
	private ArrayList<Ator> temIssoNoNome(String isso){
		ArrayList<Ator> aux = new ArrayList<>();
		for(Ator ator: elenco){
			if(ator.toString()==isso){
				aux.add(ator);
			}
		}
		return aux;
	}
}