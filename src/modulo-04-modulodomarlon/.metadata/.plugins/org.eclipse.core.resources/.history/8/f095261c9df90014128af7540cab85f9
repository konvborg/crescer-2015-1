import java.util.ArrayList;

public class Filme {
	ArrayList<Ator> elenco = new ArrayList<>();
	String titulo;
	
	public Filme(String nome){
		this.nome=nome;
	}
	
	public String toString(){
		String aux = "Titulo: ".concat(this.titulo.concat("\nElenco:"));
		for(Ator ator: elenco){
			aux.concat(" ".concat(ator.toString()));
		}
		return aux;
	}
	private void inserirAtor(Ator ator){
		this.elenco.add(ator);
	}
}