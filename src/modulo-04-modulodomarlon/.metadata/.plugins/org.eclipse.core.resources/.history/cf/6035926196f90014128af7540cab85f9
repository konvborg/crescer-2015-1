package listator;

import java.io.File;

public class Listator {
	public static void main(String[] args) {
		
	}

	private void listaDiretorios(){
		File dir = new File("C:\\Teste");
		File[] filesList = dir.listFiles();
		for (File file : filesList) {
		    if (file.isFile()) {
		        System.out.println(file.getName());
		    }
		}
	}
}

